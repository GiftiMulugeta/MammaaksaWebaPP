@model MammaaksaApp.Models.Mamaaksa

@{
    ViewData["Title"] = "Ida;i";
}

<h1>Mammaksa Ida'i</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="gaaffii" class="control-label"></label>
                <input asp-for="gaaffii" class="form-control" />
