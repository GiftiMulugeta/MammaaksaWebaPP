            </div>
            <div class="form-group">
                <p></p>
                <input type="submit" value="Ida'i'" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<p></p>
<div>
    <a asp-action="Index"  style="margin:300px;">Debbi'i </a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
